{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "automerge": true,
      "matchDatasources": [
        "crate"
      ],
      "matchDepNames": [
        "clap",
        "tokio",
        "toml",
        "regex"
      ],
      "matchUpdateTypes": [
        "patch",
        "minor"
      ]
    },
    {
      "automerge": true,
      "matchDatasources": [
        "crate"
      ],
      "matchDepNames": [
        "axum",
        "prometheus-client",
        "rustic_core",
        "rustic_backend",
        "serde",
        "tracing",
        "tracing-subscriber"
      ],
      "matchUpdateTypes": [
        "patch"
      ]
    }
  ]
}